{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": [
      "app"
    ],
    "resources": [
      "Config.toml",
      "../.env"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "olly",
  "mainBinaryName": "olly",
  "version": "0.1.0",
  "identifier": "com.olly.app",
  "plugins": {},
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "",
        "width": 800,
        "height": 600,
        "decorations": true,
        "transparent": true,
        "titleBarStyle": "Overlay",
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}